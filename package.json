    "bin": "rec",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./src/hooks/command-not-found.hook"
    }
  },
  "repository": "git://github.com/edamameldn/yggy.git",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
